<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
    <servlet>
      <servlet-name>LogDump</servlet-name>
      <servlet-class>nl.tudelft.ewi.st.atlantis.tudelft.logdump.LogDumper</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogDump</servlet-name>
        <url-pattern>/logdump</url-pattern>
    </servlet-mapping>

	<servlet>
      <servlet-name>StaticImporter</servlet-name>
      <servlet-class>nl.tudelft.ewi.st.atlantis.tudelft.logdump.StaticImporter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StaticImporter</servlet-name>
        <url-pattern>/staticimporter</url-pattern>
    </servlet-mapping>
</web-app>